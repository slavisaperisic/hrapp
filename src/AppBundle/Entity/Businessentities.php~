<?php

namespace AppBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Businessentities
 *
 * @ORM\Table(name="BusinessEntities")
 * @ORM\Entity
 */
class Businessentities
{
    /**
     * @var string
     *
     * @ORM\Column(name="BusEntType", type="string", length=1, nullable=false)
     */
    private $busenttype;

    /**
     * @var string
     *
     * @ORM\Column(name="BusEntName", type="string", length=100, nullable=false)
     */
    private $busentname;

    /**
     * @var string
     *
     * @ORM\Column(name="Gender", type="string", length=1, nullable=true)
     */
    private $gender;

    /**
     * @var integer
     *
     * @ORM\Column(name="Age", type="smallint", nullable=true)
     */
    private $age;

    /**
     * @var string
     *
     * @ORM\Column(name="FirstName", type="string", length=50, nullable=true)
     */
    private $firstname;

    /**
     * @var string
     *
     * @ORM\Column(name="MidName", type="string", length=50, nullable=true)
     */
    private $midname;

    /**
     * @var string
     *
     * @ORM\Column(name="LastName", type="string", length=50, nullable=true)
     */
    private $lastname;

    /**
     * @var string
     *
     * @ORM\Column(name="OrgName", type="string", length=100, nullable=true)
     */
    private $orgname;

    /**
     * @var integer
     *
     * @ORM\Column(name="BusEntID", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $busentid;



    /**
     * Set busenttype
     *
     * @param string $busenttype
     *
     * @return Businessentities
     */
    public function setBusenttype($busenttype)
    {
        $this->busenttype = $busenttype;

        return $this;
    }

    /**
     * Get busenttype
     *
     * @return string
     */
    public function getBusenttype()
    {
        return $this->busenttype;
    }

    /**
     * Set busentname
     *
     * @param string $busentname
     *
     * @return Businessentities
     */
    public function setBusentname($busentname)
    {
        $this->busentname = $busentname;

        return $this;
    }

    /**
     * Get busentname
     *
     * @return string
     */
    public function getBusentname()
    {
        return $this->busentname;
    }

    /**
     * Set gender
     *
     * @param string $gender
     *
     * @return Businessentities
     */
    public function setGender($gender)
    {
        $this->gender = $gender;

        return $this;
    }

    /**
     * Get gender
     *
     * @return string
     */
    public function getGender()
    {
        return $this->gender;
    }

    /**
     * Set age
     *
     * @param integer $age
     *
     * @return Businessentities
     */
    public function setAge($age)
    {
        $this->age = $age;

        return $this;
    }

    /**
     * Get age
     *
     * @return integer
     */
    public function getAge()
    {
        return $this->age;
    }

    /**
     * Set firstname
     *
     * @param string $firstname
     *
     * @return Businessentities
     */
    public function setFirstname($firstname)
    {
        $this->firstname = $firstname;

        return $this;
    }

    /**
     * Get firstname
     *
     * @return string
     */
    public function getFirstname()
    {
        return $this->firstname;
    }

    /**
     * Set midname
     *
     * @param string $midname
     *
     * @return Businessentities
     */
    public function setMidname($midname)
    {
        $this->midname = $midname;

        return $this;
    }

    /**
     * Get midname
     *
     * @return string
     */
    public function getMidname()
    {
        return $this->midname;
    }

    /**
     * Set lastname
     *
     * @param string $lastname
     *
     * @return Businessentities
     */
    public function setLastname($lastname)
    {
        $this->lastname = $lastname;

        return $this;
    }

    /**
     * Get lastname
     *
     * @return string
     */
    public function getLastname()
    {
        return $this->lastname;
    }

    /**
     * Set orgname
     *
     * @param string $orgname
     *
     * @return Businessentities
     */
    public function setOrgname($orgname)
    {
        $this->orgname = $orgname;

        return $this;
    }

    /**
     * Get orgname
     *
     * @return string
     */
    public function getOrgname()
    {
        return $this->orgname;
    }

    /**
     * Get busentid
     *
     * @return integer
     */
    public function getBusentid()
    {
        return $this->busentid;
    }
}
